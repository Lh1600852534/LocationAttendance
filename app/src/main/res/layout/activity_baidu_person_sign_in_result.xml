<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lh.henu.edu.cn.locationattendance.sign.BaiduPersonSignInResultActivity">
    <RelativeLayout
        android:id="@+id/change_titleHead"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:background="@color/colorPrimary">
        <ImageView
            android:id="@+id/baidu_person_sign_in_result_back_imageview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_back"/>
        <TextView
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="签到记录"
            android:textSize="20dp"
            android:textColor="#fff"/>
    </RelativeLayout>
    <com.baidu.mapapi.map.MapView
        android:id="@+id/baidu_person_sign_in_result_map_mapview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.baidu.mapapi.map.MapView>
</LinearLayout>
